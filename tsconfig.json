{
  "compilerOptions": {
    "noEmit": true,
    "module": "preserve",
    "target": "es2021",
    "lib": [
      "es2021"
    ],
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "noImplicitAny": false
  },
  "exclude": [
    "node_modules/"
  ]
}
