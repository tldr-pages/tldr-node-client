{
  "name": "tldr",
  "version": "0.0.6",
  "description": "Simplified and community-driven man pages",
  "author": "Romain Prieto",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rprieto/tldr.git"
  },
  "keywords": [
    "man",
    "unix",
    "commands"
  ],
  "main": "./bin/tldr",
  "bin": {
    "tldr": "./bin/tldr"
  },
  "preferGlobal": true,
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "NODE_ENV=development node server.js",
    "example": "NODE_ENV=development node ./bin/tldr tar",
    "test": "./node_modules/.bin/mocha"
  },
  "os": [
    "!win32"
  ],
  "dependencies": {
    "async": "~0.2.9",
    "commander": "~2.1.0",
    "marked": "~0.3.1",
    "mkdirp": "~0.3.5",
    "ms": "~0.6.2",
    "request": "~2.34.0",
    "rimraf": "~2.2.6",
    "unhtml": "~0.1.0",
    "unzip": "~0.1.9",
    "lodash": "~2.4.1",
    "deep-extend": "~0.2.8",
    "colors": "~0.6.2"
  },
  "devDependencies": {
    "mocha": "~1.18.0",
    "should": "~2.1.1",
    "sinon": "~1.9.0"
  }
}
