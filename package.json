{
  "name": "tldr",
  "version": "3.4.0",
  "description": "Simplified and community-driven man pages",
  "author": "Romain Prieto",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tldr-pages/tldr-node-client.git"
  },
  "keywords": [
    "man",
    "pages",
    "cheatsheets",
    "examples",
    "help",
    "unix",
    "linux",
    "osx",
    "openbsd",
    "freebsd",
    "netbsd",
    "commands",
    "command-line",
    "shell",
    "bash",
    "zsh"
  ],
  "homepage": "https://tldr.sh",
  "engines": {
    "node": ">=22"
  },
  "main": "bin/tldr",
  "files": [
    "bin",
    "config.json",
    "lib",
    "LICENSE.md"
  ],
  "bin": {
    "tldr": "bin/tldr"
  },
  "preferGlobal": true,
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "node ./bin/tldr",
    "example": "node ./bin/tldr tar",
    "test": "mocha",
    "test:quiet": "mocha --reporter=dot",
    "lint": "eslint lib test bin/tldr",
    "watch": "mocha --reporter=min --watch --growl",
    "test:functional": "bash test/functional-test.sh",
    "test:coverage": "nyc mocha",
    "test:all": "npm run lint && npm test && npm run test:functional",
    "prepare": "husky"
  },
  "dependencies": {
    "adm-zip": "^0.5.10",
    "axios": "^1.6.0",
    "commander": "^6.1.0",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.0",
    "he": "^1.2.0",
    "lodash": "^4.17.20",
    "marked": "^4.0.10",
    "ms": "^2.1.2",
    "natural": "^8.0.1",
    "ora": "^5.1.0"
  },
  "devDependencies": {
    "eslint": "^9.14.0",
    "eslint-config-eslint": "^11.0.0",
    "husky": "^9.1.6",
    "mocha": "^11.0.1",
    "nyc": "^17.1.0",
    "should": "^13.2.3",
    "sinon": "^19.0.2"
  },
  "funding": {
    "type": "liberapay",
    "url": "https://liberapay.com/tldr-pages"
  }
}
